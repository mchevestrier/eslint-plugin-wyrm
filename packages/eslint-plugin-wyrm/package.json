{
  "name": "eslint-plugin-wyrm",
  "version": "0.0.10",
  "description": "50+ ESLint rules for your TS projects",
  "keywords": [
    "eslint",
    "eslintplugin",
    "eslint-plugin",
    "linter",
    "lint"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mchevestrier/eslint-plugin-wyrm.git"
  },
  "license": "MIT",
  "author": "Martin Chevestrier",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.mts",
      "default": "./dist/index.mjs"
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/index.mjs",
  "scripts": {
    "attw": "attw --pack .",
    "prebuild": "node scripts/generate-index-file.js",
    "build": "tsdown",
    "prefix:docs": "pnpm run build",
    "fix:docs": "eslint-doc-generator",
    "fix:format": "prettier --write .",
    "fix:js": "eslint . --fix",
    "preinit:docs": "pnpm run build",
    "init:docs": "eslint-doc-generator --init-rule-docs",
    "prelint:docs": "pnpm run build",
    "lint:docs": "eslint-doc-generator --check",
    "lint:format": "prettier --check .",
    "lint:js": "eslint .",
    "lint:md": "markdownlint-cli2",
    "publint": "publint --strict",
    "test": "vitest",
    "test:cov": "vitest --coverage",
    "test:mut": "stryker run",
    "prewatch": "pnpm run prebuild",
    "watch": "tsdown --watch"
  },
  "devDependencies": {
    "@arethetypeswrong/cli": "^0.18.2",
    "@eslint/markdown": "^7.5.1",
    "@stryker-mutator/api": "^9.5.1",
    "@stryker-mutator/core": "^9.5.1",
    "@stryker-mutator/typescript-checker": "^9.5.1",
    "@stryker-mutator/vitest-runner": "^9.5.1",
    "@typescript-eslint/rule-tester": "^8.54.0",
    "@typescript-eslint/utils": "^8.54.0",
    "@vitest/coverage-v8": "^4.0.18",
    "@vitest/ui": "^4.0.18",
    "eslint": "^9.39.2",
    "eslint-doc-generator": "^3.0.2",
    "jest-diff": "^30.2.0",
    "jiti": "^2.6.1",
    "jsonc-eslint-parser": "^2.4.2",
    "markdownlint-cli2": "^0.20.0",
    "prettier": "^3.8.1",
    "publint": "^0.3.17",
    "tsdown": "^0.20.1",
    "typescript": "~5.9.3",
    "vitest": "^4.0.18",
    "vitest-fail-on-console": "^0.10.1",
    "yaml-eslint-parser": "^2.0.0"
  },
  "peerDependencies": {
    "@typescript-eslint/utils": ">=8.46.2",
    "eslint": ">=9.38.0",
    "typescript": "^5.9.3"
  },
  "engines": {
    "node": ">=22.0.0"
  }
}
