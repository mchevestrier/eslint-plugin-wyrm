{
  "name": "eslint-plugin-wyrm",
  "version": "0.0.7",
  "description": "Some custom ESLint rules",
  "keywords": [
    "eslint",
    "eslintplugin",
    "eslint-plugin",
    "linter",
    "lint"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mchevestrier/eslint-plugin-wyrm.git"
  },
  "license": "MIT",
  "author": "Martin Chevestrier",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/index.js",
  "scripts": {
    "attw": "attw --pack .",
    "prebuild": "node scripts/generate-index-file.js",
    "build": "tsdown",
    "prefix:docs": "pnpm run build",
    "fix:docs": "eslint-doc-generator",
    "fix:format": "prettier --write .",
    "fix:js": "eslint . --fix",
    "preinit:docs": "pnpm run build",
    "init:docs": "eslint-doc-generator --init-rule-docs",
    "lint:docs": "eslint-doc-generator --check",
    "lint:format": "prettier --check .",
    "lint:js": "eslint .",
    "lint:md": "markdownlint-cli2 \"**/*.md\" \"#node_modules\" \"#CHANGELOG.md\"",
    "test": "vitest",
    "test:cov": "vitest --coverage",
    "prewatch": "pnpm run prebuild",
    "watch": "tsdown --watch"
  },
  "devDependencies": {
    "@arethetypeswrong/cli": "^0.18.2",
    "@eslint/markdown": "^7.5.1",
    "@typescript-eslint/rule-tester": "^8.46.4",
    "@typescript-eslint/utils": "^8.46.4",
    "@vitest/coverage-v8": "4.0.3",
    "@vitest/ui": "4.0.3",
    "eslint": "^9.39.1",
    "eslint-doc-generator": "^2.3.0",
    "jest-diff": "^30.2.0",
    "jiti": "^2.6.1",
    "jsonc-eslint-parser": "^2.4.1",
    "markdownlint-cli2": "^0.18.1",
    "prettier": "^3.6.2",
    "tsdown": "^0.15.12",
    "typescript": "~5.9.3",
    "vitest": "^4.0.8",
    "yaml-eslint-parser": "^1.3.0"
  },
  "peerDependencies": {
    "@typescript-eslint/utils": "^8.46.2",
    "eslint": ">=9.38.0",
    "typescript": "^5.9.3"
  },
  "engines": {
    "node": ">=22.0.0"
  }
}
